{
    "version": 2,
    "builds": [
      {
        "src": "/pages/api/calculate.ts",
        "use": "@vercel/node",
        "config": {
          "runtime": "nodejs18.x" 
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/calculate.ts"
      },
      {
        "src": "/api/calculate",
        "dest": "/api/calculate" 
      }
    ]
  }